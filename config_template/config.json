  "dns": {
    "fakeip": {
      "enabled": true,
      "inet4_range": "198.18.0.0/15",
      "inet6_range": "fc00::/18"
    },
    "servers": [
      {
        "tag": "proxyserver-dns",
        "address": "https://223.5.5.5/dns-query",
        "address_strategy": "ipv4_only",
        "detour": "节点域名解析"
      },
      {
        "tag": "domestic-dns",
        "address": "https://223.5.5.5/dns-query",
        "address_strategy": "ipv4_only",
        "detour": "国内出口"
      },
      {
        "tag": "proxy-dns",
        "address": "https://1.0.0.1/dns-query",
        "address_strategy": "ipv4_only",
        "detour": "国外出口"
      },
      {
        "tag": "fakedns",
        "address": "fakeip"
      },
      {
        "tag": "nxdomain",
        "address": "rcode://success"
      }
    ],
    "rules": [
      {
        "outbound": "any",
        "server": "proxyserver-dns"
      },
      {
        "rule_set": "广告拦截",
        "server": "nxdomain",
        "rewrite_ttl": 0,
        "disable_cache": true
      },
      {
        "rule_set": [
          "HTTPDNS-域名",
          "虎牙-PCDN｜MCDN-域名"
        ],
        "server": "nxdomain",
        "disable_cache": true
      },
      {
        "domain_suffix": [
          ".market.xiaomi.com",
          ".msftncsi.com",
          ".msftconnecttest.com",
          ".wotgame.cn",
          ".wggames.cn",
          ".wowsgame.cn",
          ".wargaming.net"
        ],
        "server": "domestic-dns",
        "disable_cache": true
      },
      {
        "domain_suffix": [
          ".steamcontent.com"
        ],
        "server": "proxy-dns",
        "disable_cache": true
      },
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "rule_set": [
          "tiktok-域名",
          "强制代理-域名",
          "大陆相关-海外服务-域名"
        ],
        "server": "fakedns",
        "rewrite_ttl": 0,
        "disable_cache": true
      },
      {
        "rule_set": [
          "tiktok-域名",
          "强制代理-域名",
          "大陆相关-海外服务-域名"
        ],
        "server": "proxy-dns"
      },      
      {
        "rule_set": [
          "推送服务-域名",
          "强制直连-域名",
          "字节跳动-域名",
          "大陆相关-域名",
          "海外公司-大陆服务-域名",
          "苹果-域名"
        ],
        "server": "domestic-dns"
      },
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "rule_set": "海外公司-域名",
        "server": "fakedns",
        "rewrite_ttl": 0,
        "disable_cache": true
      },
      {
        "rule_set": "海外公司-域名",
        "server": "proxy-dns"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "rule_set": "海外公司-域名",
            "invert": true
          },
          {
            "rule_set": "大陆相关-IP"
          }
        ],
        "server": "domestic-dns"
      }
    ],
    "reverse_mapping": false,
    "independent_cache": true,
    "strategy": "ipv4_only",
    "final": "proxy-dns"
  },
